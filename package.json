{
  "name": "strapi-python-app",
  "version": "0.1.0",
  "private": true,
  "description": "A Strapi application with Python integration",
  "scripts": {
    "build": "strapi build",
    "deploy": "strapi deploy",
    "develop": "strapi develop",
    "start": "strapi start",
    "python:install": "pip install -r requirements.txt",
    "python:chmod": "chmod +x *",
    "preinstall": "npm run python:install && npm run python:chmod",
    "strapi": "strapi"
  },
  "dependencies": {
    "@strapi/strapi": "5.8.0",
    "@strapi/plugin-users-permissions": "5.8.0",
    "@strapi/plugin-cloud": "5.8.0",
    "better-sqlite3": "11.3.0",
    "fs-extra": "^10.0.0",
    "mime-types": "^2.1.27"
  },
  "devDependencies": {},
  "engines": {
    "node": ">=18.0.0 <=22.0.0",
    "npm": ">=6.0.0"
  }
}
